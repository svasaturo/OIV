<bulkInfo xmlns="http://www.cnr.it/schema/BulkInfo_v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" cmisQueryName="cvelement:commonCongresso" cmisTypeName="P:cvelement:commonCongresso" longDescription="commonCongresso" shortDescription="commonCongresso" xsi:schemaLocation="http://www.cnr.it/schema/BulkInfo_v2 BulkInfo_v2">
	
	<fieldProperty inputType="TEXT" labelClass="control-label" name="titoloEvento" property="cvelement:titoloEvento" visible="true"><jsonlabel default="Titolo evento" key="label.cvelement.titoloEvento"/><jsonvalidator maxlengthAlfresco="250" required="true"/></fieldProperty>
		
	<fieldProperty inputType="TEXT" labelClass="control-label" name="congressoIstituzione" property="cvelement:congressoIstituzione" visible="true"><jsonlabel default="Congresso/Istituzione" key="label.cvelement.congressoIstituzione"/><jsonvalidator maxlengthAlfresco="250" required="false"/></fieldProperty>
		
